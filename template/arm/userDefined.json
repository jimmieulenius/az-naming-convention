{
  "format": {
    "$field": {
      "setAction": "combine"
    },
    "default": {
      "formatString": "{ENVIRONMENT}[{-}{ORGANIZATION}]{-}{PROJECT}[{-}{PROJECT_INSTANCE}][{-}{SUBPROJECT}][{-}{REGION}]{-}{RESOURCE}[{-}{RESOURCE_INSTANCE}][{SUBRESOURCE}][{-}{SUBRESOURCE_INSTANCE}][{-}{UNIQUE}]"
    },
    "deployment": {
      "formatString": "{ENVIRONMENT}[{-}{ORGANIZATION}]{-}{PROJECT}[{-}{PROJECT_INSTANCE}][{-}{SUBPROJECT}][{-}{REGION}][{-}{RESOURCE}][{-}{RESOURCE_INSTANCE}][{SUBRESOURCE}][{-}{SUBRESOURCE_INSTANCE}][{-}v{VERSION}][{-}{DEPLOYMENT_PHASE}][{-}{SUFFIX}][{-}{UNIQUE}]"
    },
    "subnet": {
      "formatString": "{ENVIRONMENT}{-}{PROJECT}{-}{INSTANCE}[{-}{UNIQUE}]"
    }
  },
  "placeholders": {
    "ORGANIZATION": {
      "$type": "lookup",
      "default": "draftit",
      "source": {
        "draftit": {
          "value": "dra"
        }
      }
    },
    "ENVIRONMENT": {
      "$type": "lookup",
      "$field": {
        "setAction": "replace"
      },
      "source": {
        "stage": {
          "value": "stg"
        },
        "prod": {
          "value": "prd"
        }
      }
    },
    "PROJECT": {
      "$type": "lookup",
      "default": "ju",
      "source": {
        "ju": {},
        "myFirstApp": {
          "value": "mfa"
        },
        "mySecondApp": {
          "value": "msa"
        }
      }
    },
    "SUBPROJECT": {
      "$type": "lookup",
      "source": {
        "api": {
          "value": "api"
        },
        "client": {
          "value": "clt"
        }
      }
    },
    "DEPLOYMENT_PHASE": {
      "$type": "unique",
      "length": 6,
      "seed": "aaa"
    },
    "SUFFIX": {
      "$type": "instance",
      "maxValue": 999,
      "padding": {
        "totalLength": 3
      }
    }
  }
}