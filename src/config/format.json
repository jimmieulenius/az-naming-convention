{
  "default": {
    "formatString": "{ENVIRONMENT}{-}{WORKLOAD}[{-}{INSTANCE}][{-}{REGION}]{-}{RESOURCE_TYPE}[{-}{UNIQUE}]",
    "separator": "-"
  },
  "containerApps": {
    "maxLength": 32,
    "casing": "lower"
  },
  "deployment": {
    "formatString": "{ENVIRONMENT}{-}{WORKLOAD}[{-}{INSTANCE}][{-}{REGION}][{-}{RESOURCE_TYPE}][{-}v{VERSION}][{-}{UNIQUE}]",
    "maxLength": 64
  },
  "storageAccount": {
    "separator": {
      "$value": null
    },
    "maxLength": 24,
    "casing": "lower"
  },
  "virtualMachine": {
    "formatString": "{RESOURCE}[{-}{SUBRESOURCE}]"
  }
}
